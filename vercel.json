{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/public/login.html"
    },
    {
      "src": "/login.html",
      "dest": "/public/login.html"
    },
    {
      "src": "/admin-dashboard.html",
      "dest": "/public/admin-dashboard.html"
    },
    {
      "src": "/admin-pdfjs.html",
      "dest": "/public/admin-pdfjs.html"
    },
    {
      "src": "/user.html",
      "dest": "/public/user.html"
    },
    {
      "src": "/sign-pdfjs.html",
      "dest": "/public/sign-pdfjs.html"
    },
    {
      "src": "/test-vercel-api.html",
      "dest": "/test-vercel-api.html"
    },
    {
      "src": "/admin",
      "dest": "/public/admin-pdfjs.html"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/public/admin-dashboard.html"
    },
    {
      "src": "/user",
      "dest": "/public/user.html"
    },
    {
      "src": "/sign",
      "dest": "/public/sign-pdfjs.html"
    },
    {
      "src": "/sign-pdfjs",
      "dest": "/public/sign-pdfjs.html"
    }
  ]
}