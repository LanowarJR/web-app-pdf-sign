{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/admin",
      "dest": "/public/login.html"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/public/admin-dashboard.html"
    },
    {
      "src": "/admin-pdfjs",
      "dest": "/public/admin-pdfjs.html"
    },
    {
      "src": "/user",
      "dest": "/public/user.html"
    },
    {
      "src": "/sign",
      "dest": "/public/login.html"
    },
    {
      "src": "/sign-pdfjs",
      "dest": "/public/sign-pdfjs.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}